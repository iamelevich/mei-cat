{
	"name": "@mei-cat/mei-schema-valibot",
	"version": "0.0.1",
	"module": "index.ts",
	"type": "module",
	"exports": {
		".": "./dist/index.js"
	},
  "types": "./dist/index.d.ts",
	"scripts": {
    "build": "bun run build.ts && bun run build:types",
