		"lint:write": "biome lint --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.2",
		"@types/bun": "latest",
		"ts-expect": "^1.3.0"
	},
	"peerDependencies": {
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"valibot": "^1.1.0"
	}
}
