{
	"name": "@mei-cat/mei-transformer",
	"version": "0.0.1",
	"module": "index.ts",
	"type": "module",
	"exports": {
		".": "./dist/index.js"
	},
  "types": "./dist/index.d.ts",
	"scripts": {
		"transform": "bun run files/transform.ts",
    "build": "bun run build.ts && bun run build:types",
    "build:types": "tsc ./src/index.ts --declaration --emitDeclarationOnly --outDir ./dist",
		"check": "biome check",
		"format": "biome format",
		"format:write": "biome format --write .",
		"lint": "biome lint",
		"lint:write": "biome lint --write ."
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.2",
		"@types/bun": "latest",
		"@xsd-tools/typescript": "^0.2.0",
		"ts-expect": "^1.3.0",
		"xmldom": "^0.6.0",
		"xsd-tools": "^0.2.0"
	},
	"peerDependencies": {
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"fast-xml-parser": "^5.2.5",
		"saxon-js": "^2.7.0",
		"@mei-cat/mei-schema-valibot": "workspace:*",
    "valibot": "^1.1.0"
	}
}
